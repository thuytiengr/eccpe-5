# eccpe-5#include <stdio.h>
#include <conio.h>

void main()
{
	int n, i, j, d;
	clrscr();
	printf("Nhap gia tri N : ");
	scanf("%d", &n);
	printf("Cac so nguyen to khong vuot qua %d la :\n", n);

	for (d = 0, i = 2; i <= n; i++)
	{
			printf(" %d", i);
		}
	}
	printf("\nTong so co %d so nguyen to", d);
	getch();
}
